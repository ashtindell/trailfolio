<div class="user-form">
  <%= form_for(@folio) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <div>
      <%= f.label :title, class: "control-label" %>
      <%= f.text_field :title, class: "form-control input-sm", autofocus: true  %>
      <br />
      <%= f.submit 'Save', class: "btn btn-large btn-primary btn-block" %>
    </div>
    <p class="text-center"><%= link_to "...back to previous page", :back %></p>
  <% end %>
</div>